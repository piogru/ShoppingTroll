(() => {
  document.querySelector('#remote-messages').innerHTML = '<%= j render "shared/messages" %>'
  document.querySelector('#shopping-list-items').innerHTML = '<%= j render "shopping_lists/product_list" %>'
  document.querySelector('#item-count').innerHTML = '<%= j render "shopping_lists/item_count" %>'
  const modalEl = document.querySelector('#modal-window')
  if (modalEl.classList.contains("show")) {
    const modal = bootstrap.Modal.getInstance(modalEl)
    modal.hide()
  }
})();
